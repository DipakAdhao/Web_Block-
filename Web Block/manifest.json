{
    "name": "Web Block",
    "description": "Build an Extension!",
    "version": "1.1",
    "author": "dipak",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": ["storage", "activeTab", "tabs", "webRequest", "webRequestBlocking", "<all_urls>"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Web Block"
    },
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    }
}
